{
  "expo": {
    "name": "gameTime",
    "slug": "gametime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "gametime",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gametimeapp.app",
      "buildNumber": "7",
      "infoPlist": {
        "NSContactsUsageDescription": "GameTime uses your contacts to let you invite friends to groups.",
        "NSCalendarsUsageDescription": "GameTime lets you pick times; calendar access may be used in future updates.",
        "NSUserTrackingUsageDescription": "We do not track you across apps.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.gametimeapp.app",
      "versionCode": 1,
      "permissions": [
        "READ_CONTACTS",
        "POST_NOTIFICATIONS",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-contacts",
        {
          "contactsPermission": "GameTime uses your contacts to let you invite friends to groups."
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "842750e2-95c1-46ec-bada-977be4ce08ea"
      },
      "RIOT_API_KEY": "RGAPI-a23c8c62-d570-433f-8278-8c04f6f16d29"
    }
  }
}
